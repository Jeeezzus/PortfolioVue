lor: #eee;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n/* Optional: Customize pagination and navigation buttons */\n.swiper-pagination {\n  position: absolute;\n  bottom: 10px;\n  left: 50%;\n  transform: translateX(-50%);\n}\n\n.swiper-button-next, .swiper-button-prev {\n  font-size: 18px;\n  color: #007bff;\n}\n\n\n.custom-carousel {\n  height: 500px; \n}\n\n.carou{\n  position: relative;\n  top: 10%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.home{\n  height: 90vh;\n}\n\n\n</style>"],"mappings":"AA0BA;AACA;;AAEA;AACA,SAAAA,QAAA;AACA;AACAA,QAAA;AACA;AAEA;EACAC,KAAA;IACA;MACAC,cAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,MAAAC,cAAA,GAAAC,MAAA,CAAAC,WAAA;IACA,KAAAJ,cAAA,GAAAK,IAAA,CAAAC,KAAA,CAAAJ,cAAA;EACA;EACAK,UAAA;IACA;IACA;IACA;EAAA,CACA;EACAC,MAAA;IACA,MAAAC,QAAA,GAAAC,MAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,MAAA;IACA;IACA,MAAAG,aAAA,GAAAA,CAAA;MACAF,OAAA,CAAAC,GAAA;IACA;IACA;MACAH,QAAA;MACAI;IACA;EACA;AACA;;AAEA;AACA,IAAAH,MAAA,OAAAI,MAAA;EACAC,aAAA;EAAA;EACAC,YAAA;EAAA;EACAC,IAAA;EAAA;EACAC,UAAA;IACAC,EAAA;IAAA;IACAC,SAAA;EACA;;EACAC,UAAA;IACAC,MAAA;IAAA;IACAC,MAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}