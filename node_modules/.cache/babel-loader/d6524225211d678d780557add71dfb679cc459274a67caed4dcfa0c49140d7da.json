{"ast":null,"code":"//import { Carousel3d, Slide } from 'vue-carousel-3d';\n//import SlideTmplWhite from '@/components/SlideTmplWhite.vue';\n\n// import function to register Swiper custom elements\nimport { register } from 'swiper/element/bundle';\n// register Swiper custom elements\nregister();\nimport 'swiper/css';\nexport default {\n  data() {\n    return {\n      heightInPixels: 0\n    };\n  },\n  mounted() {\n    // Calculate 70% of the viewport height in pixels\n    const viewportHeight = window.innerHeight;\n    this.heightInPixels = Math.floor(viewportHeight * 0.6);\n  },\n  components: {\n    //Carousel3d,\n    //Slide,\n    //SlideTmplWhite,\n  },\n  setup() {\n    const onSwiper = swiper => {\n      console.log(swiper);\n    };\n    const onSlideChange = () => {\n      console.log('slide change');\n    };\n    return {\n      onSwiper,\n      onSlideChange\n    };\n  }\n};","map":{"version":3,"names":["register","data","heightInPixels","mounted","viewportHeight","window","innerHeight","Math","floor","components","setup","onSwiper","swiper","console","log","onSlideChange"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <!-- Include Swiper CSS -->\n<link rel=\"stylesheet\" href=\"https://unpkg.com/swiper/swiper-bundle.min.css\">\n\n<!-- Include Swiper JS -->\n<script src=\"https://unpkg.com/swiper/swiper-bundle.min.js\"></script>\n\n  <div class=\"home\">\n    <div class=\"carou\">\n      <div class=\"swiper-container\">\n  <div class=\"swiper-wrapper\">\n    <div class=\"swiper-slide\">Slide 1</div>\n    <div class=\"swiper-slide\">Slide 2</div>\n    <div class=\"swiper-slide\">Slide 3</div>\n    <!-- Add more slides as needed -->\n  </div>\n\n  <!-- If you want pagination -->\n  <div class=\"swiper-pagination\"></div>\n\n  <!-- If you want navigation buttons -->\n  <div class=\"swiper-button-next\"></div>\n  <div class=\"swiper-button-prev\"></div>\n</div>\n    </div>\n\n  </div>\n</template>\n\n<script>\n\n//import { Carousel3d, Slide } from 'vue-carousel-3d';\n//import SlideTmplWhite from '@/components/SlideTmplWhite.vue';\n\n// import function to register Swiper custom elements\nimport { register } from 'swiper/element/bundle';\n// register Swiper custom elements\nregister();\nimport 'swiper/css';\n\nexport default {\n  data() {\n    return {\n      heightInPixels: 0,\n    };\n  },\n  mounted() {\n    // Calculate 70% of the viewport height in pixels\n    const viewportHeight = window.innerHeight;\n    this.heightInPixels = Math.floor(viewportHeight * 0.6);\n  },\n  components: {\n    //Carousel3d,\n    //Slide,\n    //SlideTmplWhite,\n  },\n  setup() {\n      const onSwiper = (swiper) => {\n        console.log(swiper);\n      };\n      const onSlideChange = () => {\n        console.log('slide change');\n      };\n      return {\n        onSwiper,\n        onSlideChange,\n      };\n    },\n};\n</script>\n\n<style>\n\n.custom-carousel {\n  height: 500px; \n}\n\n.carou{\n  position: relative;\n  top: 10%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.home{\n  height: 90vh;\n}\n\n\n</style>"],"mappings":"AA+BA;AACA;;AAEA;AACA,SAAAA,QAAA;AACA;AACAA,QAAA;AACA;AAEA;EACAC,KAAA;IACA;MACAC,cAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,MAAAC,cAAA,GAAAC,MAAA,CAAAC,WAAA;IACA,KAAAJ,cAAA,GAAAK,IAAA,CAAAC,KAAA,CAAAJ,cAAA;EACA;EACAK,UAAA;IACA;IACA;IACA;EAAA,CACA;EACAC,MAAA;IACA,MAAAC,QAAA,GAAAC,MAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,MAAA;IACA;IACA,MAAAG,aAAA,GAAAA,CAAA;MACAF,OAAA,CAAAC,GAAA;IACA;IACA;MACAH,QAAA;MACAI;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}